{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@vercel/remote-nx",
      "options": {
        "verbose": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nx/react": {
      "application": {
        "babel": true
      },
      "libary": {
        "compiler": "swc",
        "style": "none",
        "setParserOptionsProject": true,
        "buildable": true
      },
      "component": {
        "project": "ui",
        "export": true,
        "pascalCaseFiles": true,
        "pascalCaseDirectory": true,
        "style": "none"
      },
      "library": {
        "unitTestRunner": "vitest"
      }
    },
    "@nx/next": {
      "application": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "strapi",
  "targetDefaults": {
    "build": {
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": ["default", "^default"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    }
  },
  "cacheDirectory": "/tmp/nx-cache",
  "useInferencePlugins": false,
  "defaultBase": "main"
}
