{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Blog Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "regex": "[^A-Z]",
      "unique": true,
      "maxLength": 25
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "blog_tags"
    }
  }
}
