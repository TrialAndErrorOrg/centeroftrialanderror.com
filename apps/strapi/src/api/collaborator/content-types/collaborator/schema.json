{
  "kind": "collectionType",
  "collectionName": "collaborators",
  "info": {
    "singularName": "collaborator",
    "pluralName": "collaborators",
    "displayName": "Collaborator"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "logo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "teamMembers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::application.application",
      "inversedBy": "collaborators"
    },
    "url": {
      "type": "string",
      "regex": "^https?://",
      "required": true
    },
    "collaborationProject": {
      "type": "string"
    }
  }
}
