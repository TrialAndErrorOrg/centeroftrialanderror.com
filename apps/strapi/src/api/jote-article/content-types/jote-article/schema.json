{
  "kind": "collectionType",
  "collectionName": "jote_articles",
  "info": {
    "singularName": "jote-article",
    "pluralName": "jote-articles",
    "displayName": "JOTE Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "abstract": {
      "type": "richtext"
    },
    "published": {
      "type": "datetime"
    },
    "submitted": {
      "type": "datetime"
    },
    "accepted": {
      "type": "datetime"
    },
    "jote_authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::jote-author.jote-author"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "jote_article_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::jote-article-category.jote-article-category"
    },
    "doi": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "references": {
      "type": "json"
    },
    "updateId": {
      "type": "string"
    }
  }
}
