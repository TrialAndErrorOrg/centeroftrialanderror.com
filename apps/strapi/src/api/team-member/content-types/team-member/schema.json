{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "bio": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "show_pronouns": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "pronouns": {
      "type": "string",
      "regex": "((he|she|they|it)(\\/(her?|she|they|him|her|them|its?))+)",
      "required": true
    },
    "azureID": {
      "type": "string",
      "unique": true
    },
    "twitter": {
      "type": "string",
      "unique": true,
      "regex": "https:\\/\\/twitter.com\\/.*"
    },
    "github": {
      "type": "string",
      "regex": "https://github.com/.*",
      "unique": true
    },
    "orcid": {
      "type": "string",
      "regex": "https://orcid.org/.*",
      "unique": true
    },
    "personalWebsite": {
      "type": "string"
    },
    "position": {
      "type": "string",
      "required": true
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "Editorial",
        "Board",
        "IT",
        "Design",
        "Outreach",
        "Production"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "lastName"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "linkedin": {
      "type": "string",
      "regex": "https:\\/\\/(www\\.)?linkedin.com\\/in\\/.*"
    },
    "blog_author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-author.blog-author",
      "inversedBy": "team_members"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "team_members"
    }
  }
}
